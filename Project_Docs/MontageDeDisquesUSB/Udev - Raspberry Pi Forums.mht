}
.dropdown li:first-child,
.dropdown li.separator + li,
.dropdown li li {
  border-top: 0;
}
.dropdown li li:first-child {
  margin-top: 4px;
}
.dropdown li li:last-child {
  padding-bottom: 0;
}
.dropdown li li {
  padding-left: 18px;
}
.wrap .dropdown li,
.dropdown.wrap li,
.dropdown-extended li {
  white-space: normal;
}
.dropdown li.separator {
  border-top: 1px solid transparent;
  margin: 5px 20px;
