	for (i =3D 0; i < startTags.length; i++) {
		var tagLength =3D startTags[i].length;
		if (start >=3D tagLength) {
			index =3D value.lastIndexOf(startTags[i], start - tagLength);
			lastStart =3D Math.max(lastStart, index);
		}
