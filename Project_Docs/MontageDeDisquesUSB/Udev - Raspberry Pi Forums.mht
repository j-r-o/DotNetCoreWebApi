	// The offset returned here is in minutes and negated.
	var offset =3D (new Date()).getTimezoneOffset();
	var sign =3D '-';

	if (offset < 0) {
		sign =3D '+';
		offset =3D -offset;
	}

	var minutes =3D offset % 60;
	var hours =3D (offset - minutes) / 60;

	if (hours < 10) {
		hours =3D '0' + hours.toString();
	} else {
		hours =3D hours.toString();
	}

	if (minutes < 10) {
		minutes =3D '0' + minutes.toString();
	} else {
		minutes =3D minutes.toString();
	}

	var prefix =3D 'UTC' + sign + hours + ':' + minutes;
	var prefixLength =3D prefix.length;
	var selectorOptions =3D $('option', '#tz_date');
	var i;

	var $tzSelectDateSuggest =3D $('#tz_select_date_suggest');

	for (i =3D 0; i < selectorOptions.length; ++i) {
		var option =3D selectorOptions[i];

		if (option.value.substring(0, prefixLength) =3D=3D=3D prefix) {
			if ($('#tz_date').val() !=3D=3D option.value && !forceSelector) {
				// We do not select the option for the user, but notify him,
				// that we would suggest a different setting.
				phpbb.timezoneSwitchDate(true);
				$tzSelectDateSuggest.css('display', 'inline');
			} else {
				option.selected =3D true;
				phpbb.timezoneSwitchDate(!forceSelector);
				$tzSelectDateSuggest.css('display', 'none');
			}

			var suggestion =3D $tzSelectDateSuggest.attr('data-l-suggestion');

			$tzSelectDateSuggest.attr('title', suggestion.replace('%s', option.inner=
HTML));
			$tzSelectDateSuggest.attr('value', suggestion.replace('%s', option.inner=
HTML.substring(0, 9)));
			$tzSelectDateSuggest.attr('data-suggested-tz', option.innerHTML);

			// Found the suggestion, there cannot be more, so return from here.
			return;
		}
	}
